CREATE TABLE STUDENT_INFO(
    ID INT(10) PRIMARY KEY,
    NAME VARCHAR(50),
    CGPA FLOAT (10,2),
    SECTION VARCHAR(2),
    COURSE VARCHAR (10)
    );

